		"@package/icons": "workspace:*",
		"@iconify/json": "2.2.175",
		"@types/node": "^20.6.3",
		"@types/react": "^18.2.21",
		"@types/react-dom": "^18.2.7",
		"@types/uuid": "^9.0.5",
		"autoprefixer": "^10.4.15",
		"postcss": "^8.4.29",
		"tailwindcss": "^3.3.3",
		"unplugin-icons": "0.18.3",
		"vite": "^5.0.12",
		"vite-plugin-svgr": "4.2.0",
		"vitest": "0.34.6"
